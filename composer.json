{
    "name": "helsingborg-stad/api-event-manager",
    "description": "Creates a api that may be used to manage events",
    "type": "wordpress-plugin",
    "license": "MIT",
    "scripts": {
        "test": "XDEBUG_MODE=off ./vendor/bin/phpunit --testdox --no-coverage",
        "test:debug": "XDEBUG_MODE=debug ./vendor/bin/phpunit --testdox --no-coverage",
        "coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --testdox",
        "minimal": "./vendor/bin/phpunit",
        "php:lint": "./vendor/bin/phpcs -s",
        "php:fix": "./vendor/bin/phpcbf",
        "php:analyze": "vendor/bin/phpstan analyse -c phpstan.neon --memory-limit=512M",
        "i18n:make-pot": "wp i18n make-pot ./ ./languages/api-event-manager.pot --slug=api-event-manager",
        "i18n:update-po": "wp i18n update-po ./languages/api-event-manager.pot ./languages/api-event-manager-sv_SE.po",
        "i18n:make-mo": "wp i18n make-mo ./languages/api-event-manager-sv_SE.po ./languages/api-event-manager-sv_SE.mo"
    },
    "extra": {
        "hooks": {
            "commit-msg": [
                "./bin/commit-msg.sh $1"
            ]
        },
        "merge-plugin": {
            "include": [
                ".devcontainer/composer.local.json"
            ],
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": true,
            "merge-scripts": true
        }
    },
    "authors": [
        {
            "name": "Thor Brink",
            "email": "thor.brink@helsingborg.se"
        },
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.thulin@helsingborg.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "EventManager\\": "source/php/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "helsingborg-stad/acf-export-manager": ">=1.0.0",
        "helsingborg-stad/wpservice": "^2.0",
        "helsingborg-stad/acfservice": "^1.0",
        "helsingborg-stad/acf-openstreetmap-field": "dev-main"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.32.0",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0",
        "brainmaestro/composer-git-hooks": "^2.8",
        "composer/installers": "~1.0",
        "wikimedia/composer-merge-plugin": "^2.1",
        "phpstan/phpstan": "2.0.x-dev",
        "johnpbloch/wordpress-core": "dev-master",
        "php-stubs/wordpress-stubs": "dev-master",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true,
            "johnpbloch/wordpress-core-installer": true
        }
    },
    "version": "3.11.9"
}